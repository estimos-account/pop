{
    "name": "GarticPhone Mod",
    "description": "",
    "version": "1.2.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.min.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "unlimitedStorage",
        "browsingData",
        "cookies"
    ],
    "host_permissions": [
        "*://garticphone.com/"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "mod.min.js",
                "draw.js",
                "images/saveicon.svg",
                "images/loadingicon.png"
            ],
            "matches": [
                "*://garticphone.com/*"
            ]
        }
    ],
    "externally_connectable": {
        "matches": [
            "*://garticphone.com/*"
        ]
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}
